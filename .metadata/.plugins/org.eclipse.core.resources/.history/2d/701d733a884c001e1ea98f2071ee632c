package SpringJDBC.Entity;

public class StudentDB {
	private int Id;
	private String Name;
	private String City;
	private int Age;
	public StudentDB(int id, String name, String city, int age) {
		super();
		Id = id;
		Name = name;
		City = city;
		Age = age;
	}
	public StudentDB() {
		super();
		// TODO Auto-generated constructor stub
	}
	public int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	public String getName() {
		return Name;
	}
	public void setName(String name) {
		Name = name;
	}
	public String getCity() {
		return City;
	}
	public void setCity(String city) {
		City = city;
	}
	public int getAge() {
		return Age;
	}
	public void setAge(int age) {
		Age = age;
	}
	@Override
	public String toString() {
		return "StudentDB [Id=" + Id + ", Name=" + Name + ", City=" + City + ", Age=" + Age + "]";
	}
	
}
